balljoint root {
	offset 0 0 0
	boxmin -0.2 -0.3 -0.15
	boxmax 0.2 0.3 0.15
	pose 0 0.1 0
	balljoint head {
		offset 0 0.3 0
		boxmin -0.1 0 -0.1
		boxmax 0.1 0.2 0.1
	}
	balljoint arm_l{
		offset -0.3 0.15 0
		boxmin -0.05 -0.3 -0.05
		boxmax 0.05 0.1 0.05
		pose 0 0 -1.3
		balljoint elbow_l{
			offset 0 -0.3 0
			boxmin -0.05 -0.2 -0.05
			boxmax 0.05 0 0.05
			pose 0 0 -0.7
		}
	}
	balljoint arm_r{
		offset 0.3 0.15 0
		boxmin -0.05 -0.3 -0.05
		boxmax 0.05 0.1 0.05
		pose 0 0 2
		balljoint elbow_l{
			offset 0 -0.3 0
			boxmin -0.05 -0.2 -0.05
			boxmax 0.05 0 0.05
			pose 0 0 0.7
		}
	}
	balljoint hip_l {
		offset -0.1 -0.3 0
		boxmin -0.05 -0.3 -0.05
		boxmax 0.05 0 0.05
		balljoint knee_l {
			offset 0 -0.3 0
			boxmin -0.05 -0.3 -0.05
			boxmax 0.05 0 0.05
			pose 1 -3 -2
		}
	}
	balljoint hip_r {
		offset 0.1 -0.3 0
		boxmin -0.05 -0.3 -0.05
		boxmax 0.05 0 0.05
		balljoint knee_r {
			offset 0 -0.3 0
			boxmin -0.05 -0.3 -0.05
			boxmax 0.05 0 0.05
			pose 1 -3 2
		}
	}
}

